{
  "name": "Dominion_kor",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Dominion - English to Korean program",
  "background": {
    "scripts": [  
      "base.js",    
      "intrigue.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Translate!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}